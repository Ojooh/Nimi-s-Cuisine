<%if (cats && cats.length> 0) { %>
<div class="untree_co-section pt-0">
    <div class="container">
        <div class="text-center mb-5" data-aos="fade-up" data-aos-delay="0">
            <span class="subtitle">Top Food Categories</span>
            <h3>Popular <strong class="text-primary">Categories</strong></h3>
        </div>
        <div class="row align-items-stretch justify-content-center">
            <% for (var t=0; t < cats.length; t++) { %>
            <div class="col-6 col-lg-3 mb-4 mb-lg-0" data-aos="fade-up" data-aos-delay="0">
                <a href="#" class="d-flex dess-item align-items-center h-100">
                    <figure class="mr-3">
                        <% if (cats[t].img && cats[t].img !='' ) { %>
                            <img src="<%= cats[t].img %>" alt="<%= cats[t].name %> Category Image" class="img-fluid">
                        <% } else { %>
                            <img src="/img/categories/default.png" alt="<%= cats[t].name %> Category Image" class="img-fluid">
                        <% } %>
                    </figure>
                    <div>
                        <h3><%= cats[t].name %></h3>
                    </div>
                </a>
            </div>
            <% } %>
            
        </div>
    </div>
</div>
<% } %>